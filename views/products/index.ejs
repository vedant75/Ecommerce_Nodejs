<%- include('../partials/header'); -%>

<h1>Products</h1>
<a href="/products/new">Add Product</a>
<ul>
    <% for (let product of products){%>
        <li><%= product.title %></li>
        <li><%= product.price %></li>
        <li><%= product.description %></li>
        <a href="/add-to-cart/<%=product._id%>" >Add to cart</a>
    <% } %>
</ul>



